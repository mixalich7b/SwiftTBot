workspace 'SwiftTBot.xcworkspace'

target 'SwiftTBot_iOS' do
	use_frameworks!
	project 'SwiftTBot.xcodeproj'
	platform :ios, '9.0'
	pod 'ObjectMapper', :git => 'https://github.com/mixalich7b/ObjectMapper.git', :tag => '3.4.2'
end

target 'SwiftTBot_OSX' do
	use_frameworks!
	project 'SwiftTBot.xcodeproj'
	platform :osx, '10.10'
	pod 'ObjectMapper', :git => 'https://github.com/mixalich7b/ObjectMapper.git', :tag => '3.4.2'
end
